{
	"name": "laravel/laravel",
	"type": "project",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"require": {
		"php": "^7.3.0",
		"barryvdh/laravel-dompdf": "^2.0",
		"beyondcode/laravel-websockets": "^1.13",
		"bigbharatjain/laravel-clickatell": "^1.0",
		"doctrine/dbal": "^3.3",
		"fideloper/proxy": "^4.2",
		"geniusts/laravel-hijri-dates": "^1.0",
		"gladcodes/keygen": "^1.1",
		"guzzlehttp/guzzle": "^7.2",
		"intervention/image": "^2.7",
		"josiasmontag/laravel-recaptchav3": "^1.0",
		"kwn/number-to-words": "^2.2",
		"laravel/framework": "^8.0",
		"laravel/helpers": "^1.5",
		"laravel/passport": "^10.0",
		"laravel/tinker": "^2.0",
		"laravel/ui": "3.0",
		"laravelcollective/html": "^6.3",
		"maatwebsite/excel": "^3.1",
		"mike42/escpos-php": "^3.0",
		"milon/barcode": "^9.0",
		"nicmart/numbers": "^0.1.0",
		"orangehill/iseed": "^2.6",
		"phpoffice/phpspreadsheet": "^1.25",
		"predis/predis": "^2.2",
		"pusher/pusher-php-server": "^7.0",
		"rachidlaasri/laravel-installer": "^4.1",
		"rap2hpoutre/laravel-log-viewer": "^2.2",
		"spatie/laravel-medialibrary": "^8.0.0",
		"spatie/laravel-permission": "^3.13",
		"stripe/stripe-php": "^7.52",
		"twilio/sdk": "^6.35"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.6",
		"facade/ignition": "^2.3.6",
		"filp/whoops": "^2.14",
		"jasonmccreary/laravel-test-assertions": "^1.0",
		"laracasts/generators": "^2.0",
		"laravel-shift/blueprint": "^1.15",
		"laravel/sail": "^1.19",
		"mockery/mockery": "^1.3.1",
		"nunomaduro/collision": "^5.0",
		"phpunit/phpunit": "^9.0",
		"symfony/thanks": "^1.2"
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"platform-check": false,
		"allow-plugins": {
			"symfony/thanks": true
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Seeders\\": "database/seeders/"
		},
		"classmap": [
			"database/seeds",
			"database/seeders",
			"database/factories"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	}
}
